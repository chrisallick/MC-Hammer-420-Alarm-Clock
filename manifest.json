{
    "name": "MC Hammer 4:20 Alarm Clock",
    "description": "Bruh. Take a moment. Whether you want to or not.",
    "version": "1.0.1",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["https://github.com/*"],
        "js": ["jq.2.min.js", "main.js"],
        "css": ["main.css"]
    }], "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ], "browser_action": {
        "default_icon": "icon128.png"
    }, "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "64": "icon64.png",
        "128": "icon128.png"
    }, "web_accessible_resources": ["hammer.gif", "stop.m4a"]
}